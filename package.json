    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "mocha": "^11.0.1",
    "nodemon": "^3.1.7",
    "nyc": "^17.1.0",
    "openid-client": "^6.8.2",
    "prettier": "^3.3.3",
