{
  "name": "@node-in-layers/auth",
  "type": "module",
  "version": "1.0.0",
  "description": "A Node In Layers Package, generated by the Node In Layers Toolkit.",
  "main": "index.js",
  "scripts": {
    "build": "rm -Rf ./dist && tsc -p ./tsconfig.json && cp package.json ./dist && cp README.md ./dist",
    "build:docs": "npx typedoc",
    "build:watch": "nodemon -e '*' --watch ./src --exec \"npm run build || exit 1\"",
    "commit": "cz",
    "dist": "npm run build && cd dist && npm publish",
    "eslint": "eslint .",
    "prettier": "prettier --write .",
    "test": "mocha -r tsx ./test/src/*.test.ts ./test/src/**/*.test.ts ./test/src/**/**/*.test.ts",
    "test:coverage": "c8 --all --reporter cobertura --reporter text --reporter lcov --reporter html npm run test",
    "test:features": "NODE_OPTIONS='--import tsx' ./node_modules/.bin/cucumber-js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "author": "",
  "license": "ISC",
  "overrides": {
    "external-editor": {
      "tmp": ">=0.2.4"
    },
    "commitizen": {
      "lodash": ">=4.17.23",
      "inquirer": {
        "external-editor": {
          "tmp": ">=0.2.4"
        }
      }
    },
    "mocha": {
      "diff": ">=8.0.3"
    },
    "sinon": {
      "diff": ">=8.0.3"
    },
    "@cucumber/cucumber": {
      "tmp": ">=0.2.4"
    }
  },
  "devDependencies": {
    "@cucumber/cucumber": "11.0.1",
    "@eslint/compat": "^1.2.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@node-in-layers/data": "^1.3.4",
    "@node-in-layers/mcp-server": "^2.5.2",
    "@types/chai-as-promised": "^8.0.1",
    "@types/json-stringify-safe": "^5.0.3",
    "@types/lodash": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^22.9.0",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "8.13.0",
    "@typescript-eslint/parser": "8.13.0",
    "argparse": "^2.0.1",
    "c8": "^11.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
